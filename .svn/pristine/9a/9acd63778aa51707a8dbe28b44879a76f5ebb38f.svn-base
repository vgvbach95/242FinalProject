#header-body {
    width: 100%;
    height: 10%;
    min-height: 50px;
    padding: 10px 10px 0px;
    background-color: #f8f8f8;
    position: relative;
}

#brand{
    font-family: 'Fira Sans', sans-serif;
    font-weight: 700;
    font-size: 40px;
    height: 100%;
    vertical-align: middle;
}

#divider {
    border: 0;
    padding: 0;
    height: 10px;
    background-color: #e7e7e7;
}

#secondary-catagory {
    vertical-align: middle;
    display: inline-block;
    position:relative;
}

.select-style {
    border: 1px solid #e7e7e7;
    width: 120px;
    border-radius: 3px;
    overflow: hidden;
    margin: auto;
    background: #fafafa url("img/icon-select.png") no-repeat 90% 50%;
}

.select-style select {
    padding: 5px 8px;
    width: 130%;
    border: none;
    box-shadow: none;
    background: transparent;
    background-image: none;
    -webkit-appearance: none;
}

.select-style select:focus {
    outline: none;
}

.navbar {
    margin-bottom: 0px;
}

.navbar-nav {
    position: absolute;
    bottom: 0;
    right: 0;
}

.navbar-nav>li>a {
    padding-top:10px;
    padding-bottom: 10px;
}

.nav>li>a {
    padding: 10px 10px;
}

.tabs {
    border: 2px solid #777;
    margin: 0px 1px;
    border-bottom: none;
}

.tabs:hover{
    border: 2px solid #e7e7e7;;
    border-bottom: none;
}

.navbar-nav>li>a {
    color: white; /* Need to fix */
}